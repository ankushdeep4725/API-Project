                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-xs-12  wow fadeInLeft">
                    <div class="faq-wrap">
                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                            @{
                                int counter = 1;
                                foreach (var item in Model)
                                {

                                    if (counter == 1)
                                    {
                                        <div class="panel panel-default">

                                            <div class="panel-heading" id="heading_@counter">
                                                <h4 class="panel-title">
                                                    <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion"
                                                       href="#collapse_@counter" aria-expanded="true" aria-controls="collapse_@counter">
                                                        @item.question
                                                    </a>
                                                </h4>
                                            </div>
                                            <div id="collapse_@counter" class="panel-collapse collapse in">
                                                <div class="panel-body">
                                                    <p style="text-align:justify;">
                                                        @item.answer
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="panel panel-default">

                                            <div class="panel-heading" id="heading_@counter">
                                                <h4 class="panel-title">
                                                    <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion"
                                                       href="#collapse_@counter" aria-expanded="false" aria-controls="collapse_@counter">
                                                        @item.question
                                                    </a>
                                                </h4>
                                            </div>
                                            <div id="collapse_@counter" class="panel-collapse collapse">
                                                <div class="panel-body">
                                                    <p style="text-align:justify;">
                                                        @item.answer
                                                    </p>
                                                </div>
                                            </div>
                                        </div>

                                    }

                                    counter = counter + 1;
                                }
                            }
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-xs-12 fadeInRight wow">
                    <div class="faq-form form-style">
                        <div class="cf-msg"></div>
                        <form action="/Home/FAQ" method="post">
                            <div class="row">
                                <div class="col-xs-12">
                                    @{
                                        if(ViewBag.SendMessage != null && ViewBag.SendMessage != "")
                                        {
                                            <div class="alert alert-success hideMe">Our Bank Executive will be in touch with you shortly. Thanks!!!</div>
                                        }
                                    }
                                </div>

                                    <div class="col-xs-12">
                                        <span>Name</span>
                                        <input type="text" placeholder="Name" id="fname" name="fname">
                                    </div>
                                    <div class="col-xs-12">
                                        <span>Email</span>
                                        <input type="text" placeholder="Email" id="email" name="email">
                                    </div>
                                    <div class="col-xs-12">
                                        <span>Subject</span>
                                        <input type="text" placeholder="Subject" id="subject" name="subject">
                                    </div>
                                    <div class="col-xs-12">
                                        <span>Your Questions</span>
                                        <textarea class="contact-textarea" placeholder="Questions" id="msg" name="msg"></textarea>
                                    </div>
                                    <div class="col-xs-12">
                                        <button id="submit" class="cont-submit btn-contact btn-style" type="submit" name="submit">Questions</button>
                                    </div>
                                </div>
</form>
                    </div>
                </div>
            </div>
        </div>
    </div>
